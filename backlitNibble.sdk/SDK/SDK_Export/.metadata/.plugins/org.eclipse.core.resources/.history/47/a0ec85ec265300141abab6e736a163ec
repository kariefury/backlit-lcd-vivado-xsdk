#include <stdio.h>
#include "xparameters.h"
#include "xil_cache.h"
#include "xintc.h"
#include "intc_header.h"
#include "xtmrctr.h"
#include "tmrctr_header.h"
#include "tmrctr_intr_header.h"
#include "backlitLCD.h"

int main() 
{
   static XIntc intc;
   static XTmrCtr axi_timer_0_Timer;
   Xil_ICacheEnable();
   Xil_DCacheEnable();
   print("---Entering main---\n\r");
   
   {
	   lcd myLCD;
	   myLCD.write = 1;
	   myLCD.db = 0x0;
	   myLCD.level = 0xFF;

	   INIT_LCD (myLCD);
   }

   print("---Exiting main---\n\r");
   Xil_DCacheDisable();
   Xil_ICacheDisable();
   return 0;
}
